<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!-- 邮件服务器配置 -->
    <bean id="emailService" class="cn.linkbook.registry.biz.service.impl.EmailServiceImpl">
        <property name="host" value="${emai.server.host}"/>
        <property name="port" value="${email.server.port}"/>
        <property name="userName" value="${email.server.username}"/>
        <property name="code" value="${email.server.code}"/>
    </bean>


</beans>